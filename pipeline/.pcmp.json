{
  "openComposedOutput": true,

  "deployment": {
    "enabled": true,
    "github": {
      "enabled": true,
      "owner": "biggaboy212",
      "repo": "portable-vide"
    }
  },

  "buildConfigs": [
    {
      "name": "Debug",

      "promptVersion": false,
      "promptDeploy": false,

      "frame": "pipeline/frame.luau",
      "darkluaConfig": "pipeline/darklua_cfgs/unminified.json",
      "input": "init.luau",
      "output": "generated/debug/vide.luau"
    },

    {
      "name": "Beta",

      "promptVersion": true,
      "promptDeploy": true,
      "prerelease": true,

      "frame": "pipeline/frame.luau",
      "darkluaConfig": "pipeline/darklua_cfgs/stable.json",
      "input": "init.luau",
      "output": "generated/beta/vide.luau",

      "variants": [
        {
          "name": "Typed",
          "darkluaConfig": "pipeline/darklua_cfgs/typed.json",
          "output": "generated/beta/vide-typed.luau"
        },
        {
          "name": "Unminified",
          "darkluaConfig": "pipeline/darklua_cfgs/unminified.json",
          "output": "generated/beta/vide-unminified.luau"
        }
      ]
    },

    {
      "name": "Release",

      "promptVersion": true,
      "promptDeploy": true,

      "frame": "pipeline/frame.luau",
      "darkluaConfig": "pipeline/darklua_cfgs/stable.json",
      "input": "init.luau",
      "output": "generated/release/vide.luau",

      "variants": [
        {
          "name": "Typed",
          "darkluaConfig": "pipeline/darklua_cfgs/typed.json",
          "output": "generated/release/vide-typed.luau"
        },
        {
          "name": "Unminified",
          "darkluaConfig": "pipeline/darklua_cfgs/unminified.json",
          "output": "generated/release/vide-unminified.luau"
        }
      ]
    }
  ]
}
